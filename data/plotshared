set terminal postscript eps enhanced color font 'Helvetica,20' linewidth 2
set output 'logoot.eps'

set key inside left top vertical enhanced autotitles nobox font ",18"

set xrange[1:1000000]
set logscale x 10
set xlabel "sequence size (log_{10})" offset 0.3

set yrange[0:450]
set ylabel "average bit-length of identifiers"

plot "logoot.dat" using 1:2 title "random" with linespoint, \
     "logoot.dat" using 1:3 title "at the end" with linespoint, \
     "logoot.dat" using 1:4 title "in front" with linespoint

set output 'robin.eps'

plot "robin.dat" using 1:2 title "random" with linespoint, \
     "robin.dat" using 1:3 title "at the end" with linespoint, \
     "robin.dat" using 1:4 title "in front" with linespoint

set output 'double.eps'

plot "double.dat" using 1:2 title "random" with linespoint, \
     "double.dat" using 1:3 title "at the end" with linespoint, \
     "double.dat" using 1:4 title "in front" with linespoint

set output 'lseq.eps'

plot "lseq.dat" using 1:2 title "random" with linespoint, \
     "lseq.dat" using 1:3 title "at the end" with linespoint, \
     "lseq.dat" using 1:4 title "in front" with linespoint
